/**app.wxss**/
@import '/iconfont/iconfont.wxss';

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


checkbox .wx-checkbox-input{
    border-radius: 11px;
    height: 20px;
    width: 20px;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
    border-radius: 14px;
    text-align: center;
    line-height: 28px;
    font-size: 16px;
    height: 28.5px;
    width: 28px;
    background-color: #1EC47C;
    color:white;
    margin-top: -1rpx;
}
/* pages/idxprogrspeed/idxprogrspeed.wxss */

.accelerate {
  width: 750rpx;
}

.accelerate .data {
  width: 100%;
  height: 200rpx;
  display: flex;
  border-bottom: 1rpx solid #eee;
}

.data .img {
  width: 140rpx;
  height: 120rpx;
}

.img image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin-top: 25rpx;
}

.data .user {
  width: 230rpx;
  height: 150rpx;
  margin-top: 20rpx;
  margin-left: 10rpx;
}

.user .name {
  width: 200rpx;
  font-size: 25rpx;
  color: #333;
  display: flex;
  margin-top: 10rpx;
}

.name image {
  width: 35rpx;
  height: 35rpx;
}

.name text {
  display: block;
  margin-top: 5rpx;
  padding-left: 5rpx;
}

.time {
  width: 320rpx;
  font-size: 27rpx;
  margin-left: 5rpx;
  margin-top: 20rpx;
}

.time .shij {
  text-align: center;
}

.time .quan {
  width: 300rpx;
  margin-left: 15rpx;
  display: flex;
}

.quan image {
  width: 35rpx;
  height: 35rpx;
  margin-top: 20rpx;
}

.quan .yhq {
  margin-top: 20rpx;
  margin-left: 10rpx;
}

.yhq .jg {
  color: red;
}
.ac_dp {
  display: flex;
}

.ac_dp image {
  width: 35rpx;
  height: 35rpx;
  margin: 15rpx 0 0 20rpx;
}
.ac_dp .dp_name{
  margin: 15rpx 0 0 15rpx;
}

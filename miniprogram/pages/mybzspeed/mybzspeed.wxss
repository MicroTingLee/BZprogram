/* pages/mybzspeed/mybzspeed.wxss */
.success {
  width: 750rpx;
}
.success .data {
  width: 100%;
  height: 200rpx;
  display: flex;
  border-bottom: 1rpx solid #eee;
}

.data .img {
  width: 140rpx;
  height: 140rpx;
}

.img image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

.data .user {
  width: 55%;
  height: 150rpx;
  margin-top: 20rpx;
  margin-left: 8rpx;
}

.user .name {
  width: 100%;
  font-size: 26rpx;
  color: #777;
  display: flex;
  margin-top: 5rpx;
}

.name image {
  width: 30rpx;
  height: 30rpx;
  margin-top: 10rpx;
}

.name text {
  display: block;
  padding-left: 5rpx;
  margin-top: 10rpx;

}
.time{
  width: 32%;
  font-size: 27rpx;
  margin-top: 20rpx;
}
.time .shij{
 text-align: center;
}
.time .quan{
width: 100%;
display: flex;
}
.quan image{
  width: 30rpx;
  height: 40rpx;
  margin-top: 15rpx;
}
.quan .yhq {
  margin-top: 15rpx;
  margin-left: 10rpx;
}
.yhq text{
  color: red;}
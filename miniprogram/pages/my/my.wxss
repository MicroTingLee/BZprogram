/* pages/my/my.wxss */
.my{
  width: 100%;
  background-color: #f2f2f2;
  /* border: 1rpx solid red; */
}
.myhead{
  height: 280rpx;
  display: flex;
  flex-wrap: nowrap;
  /* border: 1rpx solid red; */
  padding: 50rpx 40rpx 0;
  position: relative;
}
.mytouxiang{
  display: block;
  width: 110rpx;
  height: 110rpx;
  
}
.myheadbox{
  padding-left: 15rpx;
  width: 78%;
}
.myusename{
  color: #fff;
}
.membershiprank{
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  margin-right: 8rpx;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  margin-top: 10rpx;
}
.common{
  background: linear-gradient(to right,#FE97C8,#FCB1B5)
}
.glodcard{
  background: linear-gradient(to right,#FFC683,#EFD4B4)
}
.crowdfunding{
  background: linear-gradient(to right,#77BDF9,#B8C2E3);
}
.membershiprank .icon-huangguan{
  color: #fff;
  font-size: 28rpx;
} 
.membershiprank text{
  font-size: 28rpx;
}
.myrank{
  display: flex;
  flex-wrap: wrap;
}
.rktxt{
  padding-left: 10rpx;
  color: #fff;
}
/* 头部的底部 */
.headbottom1{
  width: 90%;
  position: absolute;
  left:5%;
  top: 230rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 5rpx 3rpx 20rpx #666;
}
.headbottom1 view{
  width: 49%;
  text-align: center;
}
.bttext{
  display: block;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 50rpx;
}
.headbottom1 navigator{
  height: 80rpx;
  line-height: 70rpx;
}
.headbottom2{
  background-color: #65E0D5;
  width: 90%;
  margin: 30rpx auto 0;
  height: 110rpx;
  color: #fff;
  border-radius:0 0 20rpx 20rpx;
}
.headbottom2>navigator{
  padding: 30rpx 0;
  height: 110rpx;
  line-height: 50rpx;
  text-align: center;
  box-sizing: border-box;
}
.headbottom2>navigator>.icon-tuandui{
  font-size: 55rpx;
}
.hehuo{
  font-size: 40rpx;
}
.tequan{
  font-size: 30rpx;
  padding-left: 10rpx;
}
.tequan::after{
  font-family: 'iconfont';
  color: #fff;
  font-style: normal;
  font-size: 30rpx;
  content: '\e604'
}
/* 我的订单部分 */
.myorder{
  width: 100%;
  background: #fff;
  margin: 20rpx 0;
}
.myordername{
  width: 90%;
  margin: 0 auto;
  position: relative;
  line-height: 80rpx;
}
.myordername>text::after{
  content: "";
  position: absolute;
  width: 100%;
  height: 2rpx;
  background: #ccc;
  bottom: 0;
  left: 0;
}
.myorderbox{
  width: 90%;
  height: 140rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.myorderbox>navigator{
  width: 49%;
  /* border: 1rpx solid red; */
  text-align: center;
  display: block;
  padding: 20rpx 0 10rpx;
}
.zhifupic,.myorderpic{
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
}
.myordertxt{
  padding:10rpx 0;
  font-size:30rpx;
}
/* 我的报账 */
.mybaozhang{
  width: 100%;
  /* border: 1px solid red; */
  background-color: #fff;
}
.mybaozhang>view{
  width: 90%;
  margin: 0 auto;
}
.mybzbox{
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  font-size: 30rpx;
  height: 140rpx;
}
.mybzbox>navigator{
  width: 33%;
  text-align: center;
  padding:20rpx 0 10rpx;

}
.mybzpic{
  width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
}
.bztxt{
  /* padding:10rpx 0; */
  line-height: 60rpx;
}
/* 常用功能 */
.commonfunctions{
  margin: 20rpx 0;
  background: #fff;
}
.functionbox{
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
}
.functionbox>navigator{
  /* border: 1rpx solid red; */
  width: 33%;
  text-align: center;
  padding: 20rpx 0 10rpx;
  font-size: 30rpx; 
  height: 110rpx;
}
.fcpic{
    width: 60rpx;
  height: 60rpx;
  margin: 0 auto;
  /* border: 1px solid red; */
}
.fctxt{
 line-height: 60rpx;
}
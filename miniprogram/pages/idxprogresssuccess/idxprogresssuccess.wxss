.success {
  width: 750rpx;
}

.success .data {
  width: 100%;
  height: 200rpx;
  display: flex;
  border-bottom: 1rpx solid #eee;
}

.data .img {
  width: 140rpx;
  height: 120rpx;
}

.img image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin-top: 25rpx;
}

.data .user {
  width: 230rpx;
  height: 150rpx;
  margin-top: 20rpx;
  margin-left: 10rpx;
}

.user .name {
  width: 200rpx;
  font-size: 31rpx;
  color: #777;
  display: flex;
  margin-top: 5rpx;
}

.name image {
  width: 40rpx;
  height: 40rpx;
}

.name text {
  display: block;
  margin-top: 5rpx;
  padding-left: 5rpx;
}

}
.body{
   background: url('https://yx.lingdie.com/images/image_app/style1/beijing14.png') no-repeat center;
   background-size: 100% 100%;
   height: 100%;
}
.body .return{
  width: 750rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: #eee;
}
.body .head{
  width: 700rpx;
  height: 330rpx;
  background: url('https://yx.lingdie.com/images/image_app/style1/yaoqinghan.png') no-repeat center;
  background-size: 85%;

}

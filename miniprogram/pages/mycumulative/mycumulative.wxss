/* pages/mycumulative/mycumulative.wxss */
.mycumulative{
  width: 100%;
}
.mycumu_data{
  width: 100%;
  display: flex;
  height: 180rpx;
  border-bottom: 1rpx solid #eee;
}
.mycumu_data .mycu_img{
  width: 23%;
  height: 160rpx;
  margin: 15rpx 0 0 15rpx;
  text-align: center;
}
.mycu_img image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 10rpx;
}

.mycu_content{
  width: 72%;
  height: 150rpx;
  margin-top: 15rpx;
  font-size: 28rpx;
  color: #333;
  margin-left: 10rpx;
}
.mycu_content image{
  width: 33rpx;
  height: 33rpx;
  padding-right: 10rpx;
}
.mycu_head,.mycu_center,.mycu_bottom{
  width: 100%;
  display: flex;
  height: 50rpx;
}
.mycu_name{
  width: 40%;
  white-space: nowrap;
  display: flex;
}
.mycu_head .time{
  width: 50%;
  white-space: nowrap;
}
/* 券行 */
.mycu_center{
  margin-top: 10rpx;
}
.mycu_center text{
  color: #f00;
}